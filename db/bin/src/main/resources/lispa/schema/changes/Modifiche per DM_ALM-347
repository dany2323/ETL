####Modifica tabelle  'Release di Progetto' e â€˜Release IT' aggiunta campo TYPE RELEASE####

alter table DMALM_RELEASE_DI_PROGETTO add(TYPE_RELEASE VARCHAR2(30));
alter table DMALM_RELEASE_DI_PROGETTO_ODS add(TYPE_RELEASE VARCHAR2(30));

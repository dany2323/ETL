alter table DMALM_RELEASE_IT add(TYPE_RELEASE VARCHAR2(30));
alter table DMALM_RELEASE_IT_ODS add(TYPE_RELEASE VARCHAR2(30));
alter table T_DMALM_RELEASE_IT add(TYPE_RELEASE VARCHAR2(30));